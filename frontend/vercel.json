{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "build": {
        "env": {
            "VITE_API_URL": "https://multi-tenant-saas-1.onrender.com/api"
        }
    }
}